Option Compare Database

Dim F_DATA As TTable

' Form_Load wird nicht gebraucht, weil load_data immer aufgerufen wird dierekt am anfang
Sub load_data()
    Set F_DATA = New TTable
    Dim data As String
    Dim ID As String: ID = Me.txt_id.Value
    
    ' Getting data as a string
    data = DLookup("csv_data", "simple_testf", "id=" & ID)
    
    ' Initing the object, filling array, string, table height with and delimiters
    ' achtung bei max w und h, 0 indexed. 3 heisst 4 haha
    F_DATA.init data:=data, max_w:=3, max_h:=3, del_row:=vbCr, del_col:=","
    
    ' Get Data to fill into edit form
    With Me
        .txt_name.Value = DLookup("str_name", "simple_testf", "id=" & ID)
        .str_bemerkung.Value = DLookup("str_bem", "simple_testf", "id=" & ID)
    End With
    
    ' Fill the table in the form with the data in the F_DATA object
    update_form_from_data
    
End Sub

Private Sub btn_save_Click()
    Dim name: name = txt_name.Value
    Dim data: F_DATA.get_str_data
    Dim bem: bem = str_bemerkung.Value
    
    If IsNull(name) Or IsNull(data) Or IsNull(bem) Then
        MsgBox "Enter all fields"
        Exit Sub
        
    End If
   
    Dim sql As String
    
    sql = "Update simple_testf SET str_name='" & name & "', str_bem='" & bem & "',csv_data='" & data & "' WHERE id=" & Me.txt_id & ";"
    DoCmd.RunSQL sql
    MsgBox "sucessfully saved"
    
End Sub


Private Sub btn_menu_Click()
    DoCmd.OpenForm "menu_testf"
    DoCmd.Close acForm, Me.name
End Sub

Sub paste_form_save()
    F_DATA.update_str_data new_data:=Forms!edit_simple_testf_paste.txt_data.Value
    update_form_from_data
End Sub
Private Sub btn_paste_Click()
    DoCmd.OpenForm "edit_simple_testf_paste", acNormal
    ' Form doesn't do anything, the above function past-form-save handles the save button click
    Forms!edit_simple_testf_paste.txt_data.Value = F_DATA.get_str_data
End Sub

' Basically the temp db sub, just it reverses it, instead of splitting and then filling it takes the values and puts them together
Function update_data_from_form()

    Dim data As String: data = "" ' Empty string der dann gleich befuellt wird
    
    Dim del_row, del_col
    del_row = F_DATA.get_del_row
    del_col = F_DATA.get_del_col
    
    Dim max_h, max_w
    max_h = F_DATA.get_max_h
    max_w = F_DATA.get_max_w

    ' Holt sich die Werte
    For rc = 0 To max_h
        ' Iterating through each element in form
        For cc = 0 To max_w
            data = data & Me("str_" & rc & "_" & cc & "").Value
            
            If cc <> max_w Then
                data = data & del_col   ' delimiter um eintraege zu trennen, achtung im letzten durchlauf nicht
            End If
        Next cc
        ' Newline appenden damit daten spaeter richtig ausgelesen werden koennen
        data = data & del_row
    Next rc
    
    F_DATA.update_str_data new_data:=data
    
End Function

Function update_form_from_data()
    Dim data() As String: data = F_DATA.get_arr_data
    
    Dim max_h, max_w
    max_h = F_DATA.get_max_h
    max_w = F_DATA.get_max_w
    
    ' Filling table fields from data
    For rc = 0 To max_h
                
        ' Iterating through each element in array
        For cc = 0 To max_w
            
            Me("str_" & rc & "_" & cc & "").Value = data(rc, cc)
            
        Next cc
    Next rc
    
End Function

Function update_temptable_from_data()

    Dim ID As Integer
    ID = Me!txt_id.Value

    Dim name As String: name = Me.txt_name.Value
    Dim data() As String: data = F_DATA.get_arr_data
    Dim bem As String: bem = v_or_emp(Me.str_bemerkung.Value) ' v_or_emp siehe funcs

    ' Clear the temp db, otherwise there will be old values if theyre not overwritten
    DoCmd.RunSQL "delete from temp_testf where key='1';"
    DoCmd.RunSQL "insert into temp_testf (key) values ('1');"
    
    ' Inserting name und bem into the temp table
    sql = "update temp_testf set s_bem='" & bem & "' where key='1';" ' key=1 ist einfach die default row im temp table die immer genutzt wird
    DoCmd.RunSQL sql
    sql = "update temp_testf set s_name='" & name & "' where key='1';"
    DoCmd.RunSQL sql
    
    Dim max_h, max_w
    max_h = F_DATA.get_max_h
    max_w = F_DATA.get_max_w
    
    ' Filling table fields from data
    For rc = 0 To max_h
                
        ' Iterating through each element in array
        For cc = 0 To max_w
            
            sql = "update temp_testf set str_" & rc & "_" & cc & "='" & data(rc, cc) & "' where key='1';"
            DoCmd.RunSQL sql
        
        Next cc
        
    Next rc

End Function


Private Sub btn_preview_Click()
    ' Filling the temptable from F_DATA so the report can view it
    update_temptable_from_data
    
    '  Now open the report:
    DoCmd.OpenReport "rpt_testf", acViewPreview

End Sub
